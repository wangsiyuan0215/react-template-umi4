{
    "name": "react-app",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "max build",
        "postinstall": "max setup",
        "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
        "release:prod": "npx zx ./scripts/build.prod.mjs --quiet",
        "release:qa": "npx zx ./scripts/build.stage.mjs --quiet",
        "start": "max dev",
        "test": "umi-test",
        "test:coverage": "umi-test --coverage"
    },
    "lint-staged": {
        "*.{md,json}": [
            "prettier --write"
        ],
        "src/**/*.js?(x)": [
            "eslint --ext .js",
            "prettier --write"
        ],
        "*.ts?(x)": [
            "eslint --ext .ts",
            "prettier --parser=typescript --write"
        ],
        "src/**/*.less": [
            "prettier --write",
            "stylelint --syntax less"
        ]
    },
    "dependencies": {
